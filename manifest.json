{
  "manifest_version": 2,
  "name": "DegweeWorks",
  "description": "This extension will fuck DegreeWorks up",
  "version": "1.0",
  "browser_action": {
    "default_icon": "phelps.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://degreeworks.uncc.edu/DashboardServlet/*"
      ],
      "js": [
        "popup.js"
      ]
    }
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "Command+Shift+F"
      },
      "description": "Opens html"
    }
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "https://degreeworks.uncc.edu/DashboardServlet/"
  ],
  "content_security_policy": "script-src-elem 'self' https://code.jquery.com/jquery-3.5.1.min.js; object-src 'self'; script-src 'self' https://code.jquery.com/jquery-3.5.1.min.js"
}