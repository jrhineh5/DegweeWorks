{
  "manifest_version": 2,
  "name": "DegweeWorks",
  "description": "This extension will allow a user to visualize their degree path on DegreeWorks",
  "version": "1.0",
  "browser_action": {
    "default_icon": "dgw.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://degreeworks.uncc.edu/DashboardServlet/*"
      ],
      "js": [
        "vendor/jquery-2.2.4.min.js",
        "vendor/bootstrap-4.3.1-dist/js/bootstrap.bundle.min.js",
        "popup.js"
      ],
      "css": [
        "vendor/bootstrap-4.3.1-dist/css/bootstrap.min.css"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "Command+Shift+F"
      },
      "description": "Opens html"
    }
  },
  "permissions": []
}